{
  "model": "sae",
  "batch_size": 64,
  "learning_rate": 0.005,

  "model_config": {
    "filters": 64,
    "kernel_size": 4,
    "stride": 2,

    "use_bias": true,
    "use_tied_weights": true,

    "encoder_padding": 0,
    "decoder_padding": 0,

    "encoder_nonlinearity": "leaky_relu",
    "decoder_nonlinearity": "sigmoid",

    "sparsity": 5,
    "sparsity_output_factor": 1.0,
    "use_lifetime_sparsity": true
  }
}
